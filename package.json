    "preact-render-to-string": "^3.6.0",
    "querystring": "^0.2.0",
    "ramda": "^0.23.0",
    "react-redux": "^5.0.4",
    "react-router": "^4.1.1",
    "react-router-dom": "^4.1.1",
    "redux": "^3.6.0",
    "reselect": "^3.0.0",
    "server-destroy": "^1.0.1",
    "sinon": "^2.1.0",
    "sinon-chai": "^2.9.0",
    "tape": "^4.6.3",
    "validate.js": "^0.11.1",
