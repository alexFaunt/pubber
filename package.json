{
  "name": "pubber",
  "version": "1.0.0",
  "main": "index.js",
  "author": "Alex Faunt",
  "license": "MIT",
  "scripts": {
    "start": "npm run clean && npm run build:prod && npm run server",
    "server": "node -r dotenv/config .",
    "build:prod": "NODE_ENV=production webpack",
    "build:dev": "webpack-dev-server",
    "analyze": "NODE_ENV=analyze webpack",
    "clean": "rm -rf ./build/",
    "dev": "concurrently \"npm run server\" \"npm run build:dev\"",
    "lint": "eslint --ext .jsx,.js --ignore-path .gitignore .",
    "test": "NODE_ENV=test mocha -r ./test ./test/tests/**/*.test.*",
    "test:watch": "NODE_ENV=test mocha -r ./test ./test/tests/**/*.test.* --watch",
    "precommit": "npm run lint",
    "prepush": "npm run test"
  },
  "dependencies": {
    "@google/maps": "^0.3.1",
    "aphrodite": "^1.2.0",
    "axios": "^0.16.1",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.2",
    "babel-loader": "^6.4.1",
    "babel-plugin-module-alias": "^1.6.0",
    "babel-plugin-rewire": "^1.1.0",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.4.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "babel-register": "^6.24.1",
    "babel-rewire-plugin": "^0.1.0",
    "chai": "^3.5.0",
    "chokidar": "^1.6.1",
    "concurrently": "^3.4.0",
    "dotenv": "^4.0.0",
    "enzyme": "^2.8.2",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-import-resolver-babel-module-alias": "^1.5.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.3",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.1",
    "handlebars": "^4.0.6",
    "history": "^4.6.1",
    "husky": "^0.13.3",
    "isomorphic-fetch": "^2.2.1",
    "jsdom": "^10.0.0",
    "koa": "^2.2.0",
    "koa-route": "^3.2.0",
    "koa-static-cache": "^5.0.0",
